<table>                         
  <tr>
    <th></th>                    
    <th>Title</th>
    <th>Console</th>
    <th>Release Date <br/>(YYYY-MM-DD) </th>
  </tr>

<% games.each do |game| %>
  <tr>
  <td>
      <img src="icon_12678.png" height="42" width="42">
    </td>
    <td>
      <%= game.title%>
    </td>  
    <td>
      <%= game.console%>
    </td>  
    <td>
      <%= game.created_at.strftime("%F") %>
    </td>
</tr>
<% end %>
</table>
